{
  "steps": [
    {
      "goto": "https://movementmortgagetools.com/Default.aspx"
    },
    {
      "type": [
        "#txtLogin",
        "nam-admin"
      ]
    },
    {
      "type": [
        "#txtPassword",
        "m0vement1"
      ]
    },
    {
      "waitForSelector": "#btnLogin"
    },
    {
      "click": "#btnLogin"
    },
    {
      "waitForSelector": "#ctl00_ContentPlaceHolder1_txtSearch"
    },
    {
      "type": [
        "#ctl00_ContentPlaceHolder1_txtSearch",
        "MovementTerm"
      ]
    },
    {
      "waitForSelector": [
        "#ctl00_ContentPlaceHolder1_btnSearch_input"
      ]
    },
    {
      "click": "#ctl00_ContentPlaceHolder1_btnSearch_input"
    },
    {
      "waitForSelector": [
        ".rgMasterTable"
      ]
    },
    {
      "evaluate": [
        "(cls, firstName) => { \n  const anchors = Array.from(document.querySelectorAll(cls));\n  for (var i=0;i<anchors.length;i++) {\n    let anchor = anchors[i];\n    let name = anchor.children[3].textContent.trim().toLowerCase();\n    let reg = new RegExp(firstName, 'gi');\n    if (reg.test(name)) {\n      anchor.children[1].children[0].click();\n    }\n  }\n}",
        ".rgRow",
        "test"
      ]
    },
    {
      "waitForSelector": [
        "input.rwOkBtn",
        {
          "visible": true,
          "timeout": 5000
        }
      ]
    },
    {
      "click": "input.rwOkBtn"
    }
  ]
}